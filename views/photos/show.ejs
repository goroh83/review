<% include ../partials/header %>
<div class='container'>
    <h1><%= photo.name %></h1>
    <img src='<%= photo.image %>'>
    <p> <%= photo.desc %></p>
    
    <% photo.comments.forEach(function(comment){ %>
        <p><%= comment.author %> - <%= comment.text %></p>
    <% }) %>
</div>
<% include ../partials/footer %>